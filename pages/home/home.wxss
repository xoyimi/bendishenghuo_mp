.girds {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
}

.girds-item {
  box-sizing: border-box;
  width: 250rpx;
  height: 250rpx;
  border: 1rpx solid #eee;
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
  align-items: center;
}

.girds-item image {
  width:80rpx;
  height: 80rpx;
}
